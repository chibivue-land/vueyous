{
  "name": "@vueyous/monorepo",
  "version": "0.0.6",
  "private": true,
  "type": "module",
  "scripts": {
    "setup": "ni && tsx tools/create-vueyouse/main.ts",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "oxlint --type-aware && textlint .",
    "lint:fix": "oxlint --type-aware --fix && textlint --fix .",
    "textlint": "textlint .",
    "textlint:fix": "textlint --fix .",
    "fmt": "oxfmt",
    "fmt:check": "oxfmt --check .",
    "docs:dev": "vitepress dev packages",
    "docs:build": "vitepress build packages",
    "docs:preview": "vitepress preview packages",
    "release:major": "pnpm dlx changelogen@latest --release --major",
    "release:minor": "pnpm dlx changelogen@latest --release --minor",
    "release:patch": "pnpm dlx changelogen@latest --release --patch",
    "release": "./scripts/release.sh"
  },
  "dependencies": {
    "@vueuse/core": "^14.1.0",
    "vue": "^3.5.27"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.2.0",
    "@antfu/ni": "^28.2.0",
    "@textlint-ja/textlint-rule-preset-ai-writing": "^1.6.1",
    "@tsconfig/node24": "^24.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.0.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.2",
    "eslint-plugin-oxlint": "^1.42.0",
    "fs-extra": "^11.3.3",
    "jsdom": "^27.4.0",
    "npm-run-all2": "^8.0.4",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.42.0",
    "oxlint-tsgolint": "^0.11.2",
    "textlint": "^15.5.1",
    "textlint-rule-ja-space-between-half-and-full-width": "^2.4.2",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vitepress": "^1.6.4",
    "vue-tsc": "^3.2.2"
  },
  "packageManager": "pnpm@8.15.6"
}
